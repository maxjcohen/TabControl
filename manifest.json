{
    "manifest_version": 2,
    "name": "tabControl",
    "description": "Automatic tab closing and replacing.",
    "version": "0.0.1",
    "author": "Max&Guy",
    "applications": {
	"gecko": {
	    "id": "helloworld@yoursite.com",
	    "strict_min_version": "78.0"
	}
    },
    "background": {
	"page": "src/background.html"
    },
    "permissions": [
	"messagesRead"
    ],
    "icons": {
	"64": "images/internet.png",
	"32": "images/internet-32px.png",
	"16": "images/internet-16px.png"
    }
}
